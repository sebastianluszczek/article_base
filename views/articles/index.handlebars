<h1>Articles</h1>
<div class="row">
    {{#each articles}}
    <div class="col s12 m4">
        <div class="card">
            <div class="card-content center-align">
                <h5>{{title}}</h5>
                <p class="story-text">{{{body}}}</p>
                <br>
                <div class="chip">
                    <img src="{{user.image}}">
                    {{user.firstName}} {{user.lastName}}
                </div>
            </div>
        </div>
    </div>
    {{else}}
    <p>No articles found</p>
    {{/each}}
</div>